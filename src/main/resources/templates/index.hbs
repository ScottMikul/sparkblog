{{#partial "content"}}
    <section>
        <div class="container">
            <div class="entry-list">
                {{#each entries}}
                    <article>
                        <h2><a href="/entry/{{slug}}">{{title}}</a></h2>
                        <time datetime="2016-01-31 1:00">{{date}}</time>
                    </article>
                {{/each}}
            </div>
        </div>
    </section>
{{/partial}}


{{> base.hbs}}